<%= form_for [refinery, :counties_admin, @county] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @county,
              :include_object_name => true %>

  
  <div class='field'>
    <%= f.label :name -%>
    <%= f.text_field :name, :class => 'larger widest'  -%>

  </div>
  
  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.counties.admin.counties.county'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @county.name) %>
<% end -%>
