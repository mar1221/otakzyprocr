<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(county) -%>">
  <span class='title'>
    <%= county.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.counties_county_path(county),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_counties_admin_county_path(county),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.counties_admin_county_path(county),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => county.name),
        :method => :delete %>
  </span>
</li>